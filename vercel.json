{
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/",
      "destination": "https://gitbook-docs.frontity.org"
    },
    {
      "source": "/:match*",
      "destination": "https://gitbook-docs.frontity.org/:match*"
    }
  ],
  "redirects": [
    {
      "source": "/api-reference-1",
      "destination": "https://api.frontity.org/frontity-packages"
    },
    {
      "source": "/api-reference-1/:match*",
      "destination": "https://api.frontity.org/frontity-packages/:match*"
    },
    {
      "source": "/frontity-cli",
      "destination": "https://api.frontity.org/frontity-cli"
    },
    {
      "source": "/frontity-cli/:match*",
      "destination": "https://api.frontity.org/frontity-cli/:match*"
    },
    {
      "source": "/frontity-themes",
      "destination": "https://api.frontity.org/frontity-themes"
    },
    {
      "source": "/frontity-themes/:match*",
      "destination": "https://api.frontity.org/frontity-themes/:match*"
    },
    {
      "source": "/frontity-plugins",
      "destination": "https://api.frontity.org/frontity-plugins"
    },
    {
      "source": "/frontity-plugins/:match*",
      "destination": "https://api.frontity.org/frontity-plugins/:match*"
    },
    {
      "source": "/getting-started/connecting-to-wordpress",
      "destination": "https://docs.frontity.org/getting-started/quick-start-guide#set-your-own-wordpress-installation"
    },
    {
      "source": "/learning-frontity/code-splitting",
      "destination": "https://docs.frontity.org/performance/code-splitting"
    },
    {
      "source": "/learning-frontity/head",
      "destination": "https://docs.frontity.org/seo#the-less-than-head-greater-than-component"
    },
  ]
}
